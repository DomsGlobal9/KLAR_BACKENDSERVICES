{
    "name": "klar-hotels-unified-api",
    "version": "1.0.0",
    "description": "Unified API for Klar Hotels - Search and Booking Services",
    "main": "dist/server.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "install:all": "npm install && cd hotel-search-service && npm install && cd ../hotel-booking-service && npm install"
    },
    "keywords": [
        "hotel",
        "booking",
        "search",
        "api"
    ],
    "author": "",
    "license": "ISC",
    "type": "commonjs",
    "dependencies": {
        "axios": "^1.13.2",
        "bcrypt": "^6.0.0",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^5.2.1",
        "jsonwebtoken": "^9.0.3",
        "mongoose": "^9.1.5",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.6",
        "@types/node": "^25.0.3",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.8",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.9.3"
    }
}
